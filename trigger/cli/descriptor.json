{
  "name": "flogo-cli",
  "type": "flogo:trigger",
  "shim": "main",
  "version": "0.0.1",
  "title": "CLI Trigger",
  "description": "Simple CLI Trigger",
  "homepage": "https://github.com/project-flogo/contrib/tree/master/trigger/cli",
  "output": [
    {
      "name": "args",
      "type": "array"
    }
  ],
  "reply": [
    {
      "name": "data",
      "type": "any"
    }
  ],
  "handler": {
    "settings": [
      {
        "name": "command",
        "type": "string"
      },
      {
        "name": "default",
        "type": "boolean"
      }
    ]
  }
}
